{
    "name": "sygus-lang",
    "displayName": "SyGuS Lang",
    "description": "Syntax Highlighting for SyGuS Lib v2. language.",
    "version": "0.2.0",
    "icon": "images/vscode-sygus_128.png",
    "publisher": "victornicolet",
    "repository": {
        "type": "git",
        "url": "https://github.com/victornicolet/vscode-sygus.git"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sl",
                "aliases": [
                    "SyGuS",
                    "sl"
                ],
                "extensions": [
                    ".sl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sl",
                "scopeName": "source.sl",
                "path": "./syntaxes/sl.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "sl",
                "path": "./snippets/basic.json"
            }
        ]
    },
    "dependencies": {
        "js-yaml": "^4.1.0"
    }
}